{
  "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true
  },
  "extends": [
      "google",
      "prettier"
  ],
  "plugins": ["prettier"],
  "parserOptions": {
      "ecmaVersion": 12
  },
  "rules": {
    "prettier/prettier": ["error", {"singleQuote": true, "parser": "flow"}],
    "indent": ["error", 2],
    "require-jsdoc": 0,
    "object-curly-spacing": 0
  }
}